<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>HomeControll</servlet-name>
        <servlet-class>controll.HomeControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HangControll</servlet-name>
        <servlet-class>controll.HangControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Detail</servlet-name>
        <servlet-class>controll.Detail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailControll</servlet-name>
        <servlet-class>controll.DetailControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchControll</servlet-name>
        <servlet-class>controll.SearchControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginControll</servlet-name>
        <servlet-class>controll.LoginControll</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeControll</servlet-name>
        <url-pattern>/HomeControll</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>Detail.jsp</welcome-file>
    </welcome-file-list>
    <servlet-mapping>
        <servlet-name>HangControll</servlet-name>
        <url-pattern>/HangControll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Detail</servlet-name>
        <url-pattern>/Detail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailControll</servlet-name>
        <url-pattern>/DetailControll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchControll</servlet-name>
        <url-pattern>/SearchControll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginControll</servlet-name>
        <url-pattern>/LoginControll</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
